#7
number1=raw_input("please enter first number")
number2=raw_input("please enter second number")
counter =int(number1)
counter2=int(number2)
add=0
if int(number1)<int(number2):
	while counter <= int(number2):
		add=add+counter    
		counter = counter + 1 
	print (add)
elif int(number1)>int(number2):
	while counter2 <= int(number1):
		add=add+counter2   
		counter2 = counter2 + 1
	print (add)
#8
def lowerbase(base):
	if base=="g" or base=="G":
		print "g"
	elif base=="a" or base=="A":
		print "a"
	elif base=="c" or base=="C":
		print "c"
	else:
		print "t"

counter=0
seq=raw_input('enter sequence')
while counter< len(seq):
	lowerbase(seq[counter])
	counter=counter+1
