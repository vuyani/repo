#11
name =raw_input('enter names')
first_name=name
last_name=name
shortest=name
longest=name
while name !="":
	name =raw_input('enter names')
	if name>first_name:
		first_name=name
	print "the first name is"
	print first_name
	if name<last_name:
		last_name=name
	print "the last name is"
	print last_name
	if len(name)>len(shortest):
		shortest=name
	print "the shortest is"
	print shortest 
	if len(name)<len(longest):
		longest=name
	print "the shortest is"
	print longest
