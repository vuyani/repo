#13
status = 0
X="y"
add=0
counter=0
while X =="y":
	while status !="":
	     status = raw_input("enter individual number press enter when finished:")
	     if status !="":
	     	add=float(status)+float(add)
	     	counter=counter+1
	print "the average is", int(add)/counter
	X=raw_input("do you want to continue? ")
	
#14
phrase1=raw_input("enter phrase 1")
phrase2=raw_input("enter phrase 2")
letter1=raw_input("enter letter ")
counter1= 0
counter2= 0
occurance1=0
occurance=0
while counter1 < len(phrase1):
	if letter1==phrase1[counter1]:
		occurance=occurance+1
		counter1=counter1+1
print "number of c" occurance

while counter2 < len(phrase2):
	if letter1==phrase2[counter2]:
		occurance1=occurance1+1
		counter2=counter2+1
print "number of characters in phrase 2", occurance1

#15
status=0
X="y"
add=0
counter=0
while X !="quit":
	while status !="":
	     status = raw_input("enter individual number press enter when finished:")
	     if status !="":
	     	add=float(status)+float(add)
	     	counter=counter+1
	X=raw_input("what do you want to do? sum, mean or quit ")
	status=X
	if X=="mean":
		print "the average is", int(add)/counter
	elif X=="sum":
		print "the SUM is", int(add)
	else:
		print "good bye"
